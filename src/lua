	function GUI:CreateTab(options)
		options = Constancy:validate({
			name = "Constancy Tab",
			icon = "rbxassetid://101107267002502"
		}, options or {})
		
		local Tab = {
			Hover = false,
			Active = false
		}

		
		-- Render
		do
			-- StarterGui.Constancy.Main.Navigation.ButtonHolder.Inactive
			Tab["18"] = Instance.new("TextLabel", GUI["12"]);
			Tab["18"]["BorderSizePixel"] = 0;
			Tab["18"]["TextSize"] = 12;
			Tab["18"]["TextXAlignment"] = Enum.TextXAlignment.Left;
			Tab["18"]["BackgroundColor3"] = Color3.fromRGB(255, 255, 255);
			Tab["18"]["FontFace"] = Font.new([[rbxasset://fonts/families/SourceSansPro.json]], Enum.FontWeight.Regular, Enum.FontStyle.Normal);
			Tab["18"]["TextColor3"] = Color3.fromRGB(200, 200, 200);
			Tab["18"]["BackgroundTransparency"] = 1;
			Tab["18"]["Size"] = UDim2.new(1, 0, 0, 24);
			Tab["18"]["BorderColor3"] = Color3.fromRGB(0, 0, 0);
			Tab["18"]["Text"] = options.name;
			Tab["18"]["Name"] = [[Inactive]];


			-- StarterGui.Constancy.Main.Navigation.ButtonHolder.Inactive.UIPadding
			Tab["19"] = Instance.new("UIPadding", Tab["18"]);
			Tab["19"]["PaddingLeft"] = UDim.new(0, 28);


			-- StarterGui.Constancy.Main.Navigation.ButtonHolder.Inactive.Icon
			Tab["1a"] = Instance.new("ImageLabel", Tab["18"]);
			Tab["1a"]["BorderSizePixel"] = 0;
			Tab["1a"]["BackgroundColor3"] = Color3.fromRGB(255, 255, 255);
			Tab["1a"]["ImageColor3"] = Color3.fromRGB(200, 200, 200);
			Tab["1a"]["AnchorPoint"] = Vector2.new(0, 0.5);
			Tab["1a"]["Image"] = options.icon;
			Tab["1a"]["Size"] = UDim2.new(0, 20, 0, 20);
			Tab["1a"]["BorderColor3"] = Color3.fromRGB(0, 0, 0);
			Tab["1a"]["BackgroundTransparency"] = 1;
			Tab["1a"]["Name"] = [[Icon]];
			Tab["1a"]["Position"] = UDim2.new(0, -24, 0.5, 0);
			
			-- StarterGui.Constancy.Main.ContentContainer.HomeTab
			Tab["1d"] = Instance.new("ScrollingFrame", GUI["1c"]); 
			Tab["1d"]["BorderSizePixel"] = 0;
			Tab["1d"]["CanvasPosition"] = Vector2.new(0, 0);
			Tab["1d"]["Name"] = [[HomeTab]];
			Tab["1d"]["BackgroundColor3"] = Color3.fromRGB(255, 255, 255);
			Tab["1d"]["Selectable"] = false;
			Tab["1d"]["Size"] = UDim2.new(1, 0, 1, 0);
			Tab["1d"]["BorderColor3"] = Color3.fromRGB(0, 0, 0);
			Tab["1d"]["ScrollBarThickness"] = 0;
			Tab["1d"]["BackgroundTransparency"] = 1;
			Tab["1d"]["Visible"] = true;
			
			-- StarterGui.Constancy.Main.ContentContainer.HomeTab.UIPadding
			Tab["24"] = Instance.new("UIPadding", Tab["1d"]);
			Tab["24"]["PaddingTop"] = UDim.new(0, 1);
			Tab["24"]["PaddingRight"] = UDim.new(0, 1);
			Tab["24"]["PaddingLeft"] = UDim.new(0, 1);
			Tab["24"]["PaddingBottom"] = UDim.new(0, 1);


			-- StarterGui.Constancy.Main.ContentContainer.HomeTab.UIListLayout
			Tab["25"] = Instance.new("UIListLayout", Tab["1d"]);
			Tab["25"]["Padding"] = UDim.new(0, 6);
			Tab["25"]["SortOrder"] = Enum.SortOrder.LayoutOrder;

		end

		-- Methods
		do
			function Tab:Activate()
				if not Tab.Active then
					if GUI.CurrentTab ~= nil then
						GUI.CurrentTab:Deactivate()	
					end

					Tab.Active = true
					Constancy:tween(Tab["18"], {BackgroundTransparency = .8})
					Constancy:tween(Tab["18"], {TextColor3 = Color3.fromRGB(255, 255, 255)})
					Constancy:tween(Tab["1a"], {ImageColor3 = Color3.fromRGB(255, 255, 255)})
					Tab["1d"]["Visible"] = true;
					GUI.CurrentTab = Tab
				end
			end

			function Tab:Deactivate()
				if Tab.Active then
					Tab.Active = false
					Constancy:tween(Tab["18"], {TextColor3 = Color3.fromRGB(200, 200, 200)})
					Constancy:tween(Tab["1a"], {ImageColor3 = Color3.fromRGB(200, 200, 200)})
					Constancy:tween(Tab["18"], {BackgroundTransparency = 1})
					Tab["1d"]["Visible"] = false;
				end
			end
			

			
		end
		
		-- Logic
		do
			Tab["18"].MouseEnter:Connect(function()
				Tab.Hover = true

				if not Tab.Active then
					Constancy:tween(Tab["18"], {TextColor3 = Color3.fromRGB(255, 255, 255)})
					Constancy:tween(Tab["1a"], {ImageColor3 = Color3.fromRGB(255, 255, 255)})
				end
			end)

			Tab["18"].MouseLeave:Connect(function()
				Tab.Hover = false

				if not Tab.Active then
					Constancy:tween(Tab["18"], {TextColor3 = Color3.fromRGB(200, 200, 200)})
					Constancy:tween(Tab["1a"], {ImageColor3 = Color3.fromRGB(200, 200, 200)})
				end
			end)

			uis.InputBegan:Connect(function(input, gpe)
				if gpe then return end

				if input.UserInputType == Enum.UserInputType.MouseButton1 then
					if Tab.Hover then
						Tab:Activate()
					end
				end
			end)
		end

		if GUI.CurrentTab == nil then
			Tab:Activate()
		end
		
		function Tab:Button(options)
			options = Constancy:validate({
				text = "Constancy Button",
				callback = function() end
			}, options or {})

			local Button = {
				Hover = false,
				MouseDown = false
			}
			
			-- Render
			do
				-- StarterGui.Constancy.Main.ContentContainer.HomeTab.Button
				Button["1e"] = Instance.new("Frame", Tab["1d"]);
				Button["1e"]["BorderSizePixel"] = 0;
				Button["1e"]["BackgroundColor3"] = Color3.fromRGB(28, 28, 28);
				Button["1e"]["Size"] = UDim2.new(1, 0, 0, 32);
				Button["1e"]["BorderColor3"] = Color3.fromRGB(0, 0, 0);
				Button["1e"]["Name"] = [[Button]];


				-- StarterGui.Constancy.Main.ContentContainer.HomeTab.Button.UICorner
				Button["1f"] = Instance.new("UICorner", Button["1e"]);
				Button["1f"]["CornerRadius"] = UDim.new(0, 4);


				-- StarterGui.Constancy.Main.ContentContainer.HomeTab.Button.UIStroke
				Button["20"] = Instance.new("UIStroke", Button["1e"]);
				Button["20"]["Color"] = Color3.fromRGB(83, 83, 83);
				Button["20"]["ApplyStrokeMode"] = Enum.ApplyStrokeMode.Border;


				-- StarterGui.Constancy.Main.ContentContainer.HomeTab.Button.Title
				Button["21"] = Instance.new("TextLabel", Button["1e"]);
				Button["21"]["BorderSizePixel"] = 0;
				Button["21"]["TextSize"] = 12;
				Button["21"]["TextXAlignment"] = Enum.TextXAlignment.Left;
				Button["21"]["BackgroundColor3"] = Color3.fromRGB(255, 255, 255);
				Button["21"]["FontFace"] = Font.new([[rbxasset://fonts/families/Ubuntu.json]], Enum.FontWeight.Regular, Enum.FontStyle.Normal);
				Button["21"]["TextColor3"] = Color3.fromRGB(255, 255, 255);
				Button["21"]["BackgroundTransparency"] = 1;
				Button["21"]["Size"] = UDim2.new(1, -20, 1, 0);
				Button["21"]["BorderColor3"] = Color3.fromRGB(0, 0, 0);
				Button["21"]["Text"] = options.text;
				Button["21"]["Name"] = [[Title]];


				-- StarterGui.Constancy.Main.ContentContainer.HomeTab.Button.UIPadding
				Button["22"] = Instance.new("UIPadding", Button["1e"]);
				Button["22"]["PaddingTop"] = UDim.new(0, 6);
				Button["22"]["PaddingRight"] = UDim.new(0, 6);
				Button["22"]["PaddingLeft"] = UDim.new(0, 6);
				Button["22"]["PaddingBottom"] = UDim.new(0, 6);


				-- StarterGui.Constancy.Main.ContentContainer.HomeTab.Button.Icon
				Button["23"] = Instance.new("ImageLabel", Button["1e"]);
				Button["23"]["BorderSizePixel"] = 0;
				Button["23"]["BackgroundColor3"] = Color3.fromRGB(255, 255, 255);
				Button["23"]["AnchorPoint"] = Vector2.new(1, 0);
				Button["23"]["Image"] = [[rbxassetid://111250893869573]];
				Button["23"]["Size"] = UDim2.new(0, 20, 0, 20);
				Button["23"]["BorderColor3"] = Color3.fromRGB(0, 0, 0);
				Button["23"]["BackgroundTransparency"] = 1;
				Button["23"]["Name"] = [[Icon]];
				Button["23"]["Position"] = UDim2.new(1, 0, 0, 0);
			end
			
			-- Methods
			do
				function Button:SetText(text)
					options.text = text
					Button["21"].Text = text
				end
				
				function Button:callback(fn)
					options.callback = fn
				end
			end
			
			-- Methods
			do
				Button["1e"].MouseEnter:Connect(function()
					Button.Hover = true
					
					Constancy:tween(Button["20"], {Color = Color3.fromRGB(102, 102, 102)})
				end)
				
				Button["1e"].MouseLeave:Connect(function()
					Button.Hover = false
					
					if not Button.MouseDown then
						Constancy:tween(Button["20"], {Color = Color3.fromRGB(83, 83, 83)})
					end
				end)
				
				
				uis.InputBegan:Connect(function(input, gpe)
					if gpe then return end
					
					if input.UserInputType == Enum.UserInputType.MouseButton1 and Button.Hover then
						Button.MouseDown = true
						Constancy:tween(Button["1e"], {BackgroundColor3 = Color3.fromRGB(57, 57, 57)})
						Constancy:tween(Button["20"], {Color = Color3.fromRGB(200, 200, 200)})
						options.callback()
					end
				end)	
				
				uis.InputEnded:Connect(function(input, gpe)
					if gpe then return end

					if input.UserInputType == Enum.UserInputType.MouseButton1 then
						Button.MouseDown = false
						
						if Button.Hover then
							Constancy:tween(Button["1e"], {BackgroundColor3 = Color3.fromRGB(57, 57, 57)})
							Constancy:tween(Button["20"], {Color = Color3.fromRGB(102, 102, 102)})
						else
							Constancy:tween(Button["1e"], {BackgroundColor3 = Color3.fromRGB(28, 28, 28)})
							Constancy:tween(Button["20"], {Color = Color3.fromRGB(83, 83, 83)})
						end
					end
				end)
			end
			
			return Button
		end
		
