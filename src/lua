				Warning["29"]["BorderSizePixel"] = 0;
				Warning["29"]["TextSize"] = 12;
				Warning["29"]["TextXAlignment"] = Enum.TextXAlignment.Left;
				Warning["29"]["BackgroundColor3"] = Color3.fromRGB(255, 255, 255);
